<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="context/main_template/macros/master"
      i18n:domain="plone">

  <body>
    <metal:content-core fill-slot="main">
      <metal:block define-macro="main">
         <div id="anuncios" tal:define="catalog python:context.portal_catalog;anuncios python:catalog(portal_type='anuncio')" >
         <div   tal:attributes="class string:anuncio-ativo"  tal:repeat="anuncio anuncios" >
            <h1 tal:define="titulo python:anuncio.getTitle()" tal:content="string:${titulo}"></h1>
             
         </div>
         </div>
      </metal:block>
    </metal:content-core>
  </body>
</html>
